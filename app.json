					{
						"name": "digit_1",
						"type": "number",
						"title": {
							"en": "units",
							"nl": "eenheden"
						}
					},
					{
						"name": "digit_2",
						"type": "number",
						"title": {
							"en": "tens",
							"nl": "tientallen"
						}
					},
					{
						"name": "digit_3",
						"type": "number",
